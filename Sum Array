#include <stdio.h>

int main() {
  // Initialize the array
  int numbers[] = {2, 7, 9, 3, 6};
  int sum = 0;

  // Find the sum of all numbers on even indexes
  for (int i = 0; i < 5; i++) {
    if (i % 2 == 0) {
      sum += numbers[i];
    }
  }

  // Print the sum
  printf("The sum of all numbers on even indexes is %d\n", sum);

  return 0;
}
